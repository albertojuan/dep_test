NODE unique_daily_product_clicks_node
SQL >

    SELECT
        merchant,
        market,
        date(timestamp) AS date,
        product_id,
        request_type,
        request_query,
        uniqState(request_id) AS clicks
    FROM click
    GROUP BY
        merchant,
        market,
        date,
        product_id,
        request_type,
        request_query

TYPE materialized
DATASOURCE product_clicks_per_request_daily


